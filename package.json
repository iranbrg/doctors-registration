{
    "name": "test-grupo-gcb",
    "version": "1.0.0",
    "description": "",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon --exec ts-node src/server.ts --ignore __tests__/ --ignore prisma",
        "start": "node dist/server.js",
        "build": "rimraf ./dist && tsc",
        "lint": "eslint . --ext js,ts",
        "lint:fix": "eslint . --ext js,ts --fix",
        "format": "prettier -w '**/*.{js,ts}' '!**/coverage/**'",
        "test": "jest",
        "db:migrate:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml exec gcb_node_app npx prisma migrate dev",
        "db:migrate:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml exec gcb_node_app npx prisma migrate deploy",
        "docker:node:down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml rm -sf gcb_node_app",
        "docker:node:build": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d --build --no-deps gcb_node_app",
        "docker:node:rebuild": "npm run docker:down && npm run docker:build",
        "docker:down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down -v",
        "docker:build": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
        "docker:rebuild": "npm run docker:down && npm run docker:build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iranbrg/test-grupo-gcb.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/iranbrg/test-grupo-gcb/issues"
    },
    "homepage": "https://github.com/iranbrg/test-grupo-gcb#readme",
    "dependencies": {
        "@prisma/client": "^3.5.0",
        "dotenv": "^10.0.0",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.7",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.3.1",
        "@typescript-eslint/parser": "^5.3.1",
        "eslint": "^8.2.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.3.1",
        "nodemon": "^2.0.15",
        "prettier": "^2.4.1",
        "prisma": "^3.5.0",
        "rimraf": "^3.0.2",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    }
}
